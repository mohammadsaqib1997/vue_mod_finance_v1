#calculator.modal.fade.modal-aside.center(tabindex='-1', role='dialog', aria-labelledby='All Calculator', aria-hidden='true')
    .modal-dialog.modal-dialog.modal-lg
        .modal-content
            .modal-header
                button.close(type='button', data-dismiss='modal', aria-label='Close')
                    span(aria-hidden='true') Ã—
                h4#myModalLabel.modal-title Calculator
            .modal-body
                .panel.panel-white
                    .panel-body
                        ul.nav
                            li.col-md-6.padding-none.active
                                a(href='#simple', data-toggle='tab') Simple Calculator
                            li.col-md-6.padding-none
                                a(href='#compound', data-toggle='tab') Compound Calculator
                        .tab-content
                            #simple.tab-pane.fade.in.active
                                .row
                                    .col-md-12
                                        p.result {{ simple_res }}
                                        .calculator-section
                                            .row
                                                .col-md-4
                                                    label(style='line-height: 32px;') Enter Current Calculation Label:
                                                .col-md-8.padding-left-none
                                                    .row.margin-none.save-section
                                                        .col-md-8.padding-none
                                                            input.form-control.labelsave(type='text')
                                                        .col-md-4.padding-none
                                                            input.btn.btn-red.btn-md.full-width(type='submit', style='background: #a6c367;text-transform: uppercase;font-weight: 600;')
                                            #calculatortab1
                                                .recent-calcu
