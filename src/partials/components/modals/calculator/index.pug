#calculator.modal.fade.modal-aside.center(tabindex='-1', role='dialog', aria-labelledby='All Calculator', aria-hidden='true')
    .modal-dialog.modal-dialog.modal-lg
        .modal-content
            .modal-header
                button.close(type='button', data-dismiss='modal', aria-label='Close')
                    span(aria-hidden='true') Ã—
                h4#myModalLabel.modal-title Calculator
            .modal-body
                .panel.panel-white
                    .panel-body
                        ul.nav
                            li.col-md-6.padding-none.active
                                a(href='#simple', data-toggle='tab') Simple Calculator
                            li.col-md-6.padding-none
                                a(href='#compound', data-toggle='tab') Compound Calculator
                        .tab-content
                            #simple.tab-pane.fade.in.active
                                .row
                                    .col-md-12
                                        p.result {{ simple_res }}
                                        .calculator-section
                                            .row
                                                .col-md-4
                                                    label(style='line-height: 32px;') Enter Current Calculation Label:
                                                .col-md-8.padding-left-none
                                                    form.row.margin-none.save-section(v-on:submit.prevent='saveSimpleResult')
                                                        .col-md-8.padding-none
                                                            input.form-control.labelsave(type='text' v-model='saveSimCalName')
                                                        .col-md-4.padding-none
                                                            button.btn.btn-red.btn-md.full-width(v-if='!saveSimCalProcess' type='submit' style='background: rgb(166, 195, 103);text-transform: uppercase;font-weight: 600;border-radius: 0;width: 100%;border: none;') Submit
                                                            button.btn.btn-red.btn-md.full-width(v-else type='button' style='background: rgb(166, 195, 103);text-transform: uppercase;font-weight: 600;border-radius: 0;width: 100%;border: none;' disabled='disabled')
                                                                i.fa.fa-refresh.fa-spin
                                                        .col-md-12.padding-none
                                                            p.alert.alert-danger(v-if='validation.hasError("saveSimCalName")') {{ validation.firstError('saveSimCalName') }}
                                                            p.alert.alert-danger(v-if='saveSimCalErr !== ""') {{ saveSimCalErr }}
                                                            p.alert.alert-success(v-if='saveSimCalMsg !== ""') {{ saveSimCalMsg }}
                                            #calculatortab1
                                                .recent-calcu
                                                    .row
                                                        .col-md-6
                                                            simpleIntCal(v-on:calculate="showSimpleResult")
                                                        .col-md-6
                                                            h4.heading Recent Calculations
                                                            .recent-calculations
                                                                template(v-if='load1')
                                                                    .row.margin-none
                                                                        .col-md-12.text-center
                                                                            i.fa.fa-refresh.fa-spin.fa-3x
                                                                template(v-else)
                                                                    .row.margin-none(v-for='item in simCalData')
                                                                        .col-md-8.padding-none
                                                                            p.text-left {{ item.label }}
                                                                        .col-md-4.padding-none
                                                                            p.text-right.text-bold {{ item.result }}
                                                                //.row.margin-none.no-border
                                                                //    .col-md-12.padding-none
                                                                //        a.anchor-red.pull-right(href='#') Clear All
                            #compound.tab-pane.fade.in
                                .row
                                    .col-md-12
                                        .row
                                            .col-md-6
                                                p Future Value
                                                p.result {{ comp_fv_res }}
                                            .col-md-6
                                                p Interest Earned
                                                p.result {{ comp_iv_res }}
                                        .calculator-section
                                            .row
                                                .col-md-4
                                                    label(style='line-height: 32px;') Enter Current Calculation Label:
                                                .col-md-8.padding-left-none
                                                    form.row.margin-none.save-section(v-on:submit.prevent='saveCompoundResult')
                                                        .col-md-8.padding-none
                                                            input.form-control.labelsave(type='text' v-model='saveComCalName')
                                                        .col-md-4.padding-none
                                                            button.btn.btn-red.btn-md.full-width(v-if='!saveComCalProcess' type='submit' style='background: rgb(166, 195, 103);text-transform: uppercase;font-weight: 600;border-radius: 0;width: 100%;border: none;') Submit
                                                            button.btn.btn-red.btn-md.full-width(v-else type='button' style='background: rgb(166, 195, 103);text-transform: uppercase;font-weight: 600;border-radius: 0;width: 100%;border: none;' disabled='disabled')
                                                                i.fa.fa-refresh.fa-spin
                                                        .col-md-12.padding-none
                                                            p.alert.alert-danger(v-if='validation.hasError("saveComCalName")') {{ validation.firstError('saveComCalName') }}
                                                            p.alert.alert-danger(v-if='saveComCalErr !== ""') {{ saveComCalErr }}
                                                            p.alert.alert-success(v-if='saveComCalMsg !== ""') {{ saveComCalMsg }}
                                            #calculatortab2
                                                .recent-calcu
                                                    .row
                                                        .col-md-6
                                                            compoundIntCal(v-on:calculate="showCompoundResult")
                                                        .col-md-6
                                                            h4.heading Recent Calculations
                                                            .recent-calculations
                                                                template(v-if='load2')
                                                                    .row.margin-none
                                                                        .col-md-12.text-center
                                                                            i.fa.fa-refresh.fa-spin.fa-3x
                                                                template(v-else)
                                                                    .row.margin-none(v-for='item in comCalData')
                                                                        .col-md-8.padding-none
                                                                            p.text-left {{ item.label }}
                                                                        .col-md-4.padding-none
                                                                            p.text-right.text-bold Future Value: {{ item.result.fv }}
                                                                            p.text-right.text-bold Interest Earned: {{ item.result.totInt }}
                                                                //.row.margin-none.no-border
                                                                //    .col-md-12.padding-none
                                                                //        a.anchor-red.pull-right(href='#') Clear All
