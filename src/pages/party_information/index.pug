#container.wrap-content.container
    section#page-title
        .row
            .col-sm-8
                h1.mainTitle Process
                span.mainDescription Post/Un Post
            ol.breadcrumb
                li
                    span Process
                li.active
                    span Post/Un Post
    .container-fluid.container-fullw.bg-white
        .row
            .col-md-12
                h2 Post/Un Post
                form#form(action='#', role='form', novalidate='novalidate')
                    .row
