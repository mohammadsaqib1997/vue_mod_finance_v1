.bg-offwhite
    #container.wrap-content.container-fluid
        section#page-title
            .row
                .col-md-2.border-right(style='border-color: #aeaeae;')
                    img(src='http://45.55.84.192:3000/assets/images/fms.png', class="logo-image", style='width: 100%;max-width:230px;')
                .col-sm-8
                    .col-sm-8
                        h1.mainTitle(style='font-size: 38.02px;') #{ optionalData.title }
                        span.mainDescription(style='font-size: 19px;color: #ec5d58;') #{ optionalData.subTitle }
    .row.padding-30
        .col-md-12
            .text-center(v-if='dataLoad1')
                i.fa.fa-refresh.fa-spin.fa-3x
            .panel.panel-white.no-radius(v-else)
                .panel-heading.border-bottom.border-left.border-yellow.no-padding.no-line-height.no-min-height
                    h4.panel-title.pull-left.padding-15 Vendor Details
                    h6.pull-right.text-right.padding-15 Create Date: #{ optionalData.date }
                .panel-body.padding-30
                    .row
                        .col-xs-12.col-sm-12.col-md-12.col-lg-12
                            h3.p_subheading Person Details
                        .col-xs-12.col-sm-12.col-md-3.col-lg-3
                            .form-group
                                .row
                                    .col-xs-12.col-sm-4.col-md-4.col-lg-4
                                        label Contact Name
                                    .col-xs-12.col-sm-8.col-md-8.col-lg-8
                                        strong #{fetchData.data.contact_name}
                        .col-xs-12.col-sm-12.col-md-3.col-lg-3
                            .form-group
                                .row
                                    .col-xs-12.col-sm-4.col-md-4.col-lg-4
                                        label Agent Code
                                    .col-xs-12.col-sm-8.col-md-8.col-lg-8
                                        strong #{fetchData.data.agent_code}
                        .col-xs-12.col-sm-12.col-md-3.col-lg-3
                            .form-group
                                .row
                                    .col-xs-12.col-sm-4.col-md-4.col-lg-4
                                        label Agent Name
                                    .col-xs-12.col-sm-8.col-md-8.col-lg-8
                                        strong #{fetchData.data.agent_name}
                        .col-xs-12.col-sm-12.col-md-3.col-lg-3
                            .form-group
                                .row
                                    .col-xs-12.col-sm-4.col-md-4.col-lg-4
                                        label Category
                                    .col-xs-12.col-sm-8.col-md-8.col-lg-8
                                        strong #{fetchData.data.sel_category}
                        .col-xs-12.col-sm-12.col-md-3.col-lg-3
                            .form-group
                                .row
                                    .col-xs-12.col-sm-4.col-md-4.col-lg-4
                                        label Group Code
                                    .col-xs-12.col-sm-8.col-md-8.col-lg-8
                                        strong #{fetchData.data.group_code}
                    .row
                        .col-xs-12.col-sm-12.col-md-12.col-lg-12
                            h3.p_subheading Contact Details
                        .col-xs-12.col-sm-12.col-md-3.col-lg-3
                            .form-group
                                .row
                                    .col-xs-12.col-sm-4.col-md-4.col-lg-4
                                        label Phone Number
                                    .col-xs-12.col-sm-8.col-md-8.col-lg-8
                                        strong #{fetchData.data.phone_num}
                        .col-xs-12.col-sm-12.col-md-3.col-lg-3
                            .form-group
                                .row
                                    .col-xs-12.col-sm-4.col-md-4.col-lg-4
                                        label Fax Number
                                    .col-xs-12.col-sm-8.col-md-8.col-lg-8
                                        strong #{fetchData.data.fax_num}
                        .col-xs-12.col-sm-12.col-md-3.col-lg-3
                            .form-group
                                .row
                                    .col-xs-12.col-sm-4.col-md-4.col-lg-4
                                        label Email
                                    .col-xs-12.col-sm-8.col-md-8.col-lg-8
                                        strong #{fetchData.data.email}
                        .col-xs-12.col-sm-12.col-md-3.col-lg-3
                            .form-group
                                .row
                                    .col-xs-12.col-sm-4.col-md-4.col-lg-4
                                        label Address
                                    .col-xs-12.col-sm-8.col-md-8.col-lg-8
                                        strong #{fetchData.data.address}
                        .col-xs-12.col-sm-12.col-md-3.col-lg-3
                            .form-group
                                .row
                                    .col-xs-12.col-sm-4.col-md-4.col-lg-4
                                        label City
                                    .col-xs-12.col-sm-8.col-md-8.col-lg-8
                                        strong #{fetchData.data.city}
                    .row
                        .col-xs-12.col-sm-12.col-md-12.col-lg-12
                            h3.p_subheading Legal Information
                        .col-xs-12.col-sm-12.col-md-3.col-lg-3
                            .form-group
                                .row
                                    .col-xs-12.col-sm-4.col-md-4.col-lg-4
                                        label NIC Number
                                    .col-xs-12.col-sm-8.col-md-8.col-lg-8
                                        strong #{fetchData.data.nic_num}
                        .col-xs-12.col-sm-12.col-md-3.col-lg-3
                            .form-group
                                .row
                                    .col-xs-12.col-sm-4.col-md-4.col-lg-4
                                        label NTN Number
                                    .col-xs-12.col-sm-8.col-md-8.col-lg-8
                                        strong #{fetchData.data.ntn_num}
                        .col-xs-12.col-sm-12.col-md-3.col-lg-3
                            .form-group
                                .row
                                    .col-xs-12.col-sm-4.col-md-4.col-lg-4
                                        label GST Number
                                    .col-xs-12.col-sm-8.col-md-8.col-lg-8
                                        strong #{fetchData.data.gst_num}
                        .col-xs-12.col-sm-12.col-md-3.col-lg-3
                            .form-group
                                .row
                                    .col-xs-12.col-sm-4.col-md-4.col-lg-4
                                        label CR Day
                                    .col-xs-12.col-sm-8.col-md-8.col-lg-8
                                        strong #{fetchData.data.cr_day}
                        .col-xs-12.col-sm-12.col-md-3.col-lg-3
                            .form-group
                                .row
                                    .col-xs-12.col-sm-4.col-md-4.col-lg-4
                                        label CR Limit
                                    .col-xs-12.col-sm-8.col-md-8.col-lg-8
                                        strong #{fetchData.data.cr_limit}
                    .row
                        .col-xs-12.col-sm-12.col-md-12.col-lg-12
                            h3.p_subheading Voucher Entries
                    table.table-responsive.table.table-striped.misreport(cellpadding='5', style='width:100%;', border='1')
                        tbody
                            tr.background-e2e8f0.no-border
                                th.col-md-1 Voucher ID
                                th.col-md-1 Voucher Type
                                th.col-md-1 Voucher Date
                                th.col-md-1 Remarks
                                th.col-md-1 Project
                                th.col-md-1 Status
                            each ent, ind in fetchData.entData
                                tr
                                    td.col-md-1 #{ ent.id }
                                    td.col-md-1 JV
                                    td.col-md-1 #{ ent.voucher_date }
                                    td.col-md-1 #{ ent.v_remarks }
                                    td.col-md-1 #{ ent.sel_project }
                                    td.col-md-1 #{ ent.posted_status }